{
  "name": "gkeep",
  "displayName": "gkeep",
  "description": "An extension which adds .gitkeep file to include blank folders for git add",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.89.0"
  },
  "icon": "./icon.png",
  "categories": [
    "Other"
  ],
  "publisher": "asyncjs",
  "activationEvents": [
    "onStartup"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addGitKeep",
        "title": "add git keep"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
